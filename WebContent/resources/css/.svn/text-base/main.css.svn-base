header {
	padding-bottom: 70px;
}

.tbpergunta tr {
	margin-bottom: 100px;
}

footer {
	color: #0895ca;
	font-size: 10px;
}

nav {
	background-color: #2980b9;
}

.panel-body {
	background-color: #f7f8f9;
	padding-top: 20px;
	padding-bottom: 30px;
	-moz-border-radius: 5px; /* Para Firefox */
	-webkit-border-radius: 5px; /*Para Safari e Chrome */
	border-radius: 5px; /* Para Opera 10.5+*/
}

.panel {
	padding-bottom: 10px;
	background-color: #2980b9;
	border-width: 1px;
	border-style: solid;
	border-color: #2980b9;
	-moz-border-radius: 5px; /* Para Firefox */
	-webkit-border-radius: 5px; /*Para Safari e Chrome */
	border-radius: 5px; /* Para Opera 10.5+*/
}

.panel-heading {
	padding-bottom: 5px;
	background-color: #2980b9;
	border-width: 1px;
	border-style: solid;
	border-color: #2980b9;
	-moz-border-radius: 5px; /* Para Firefox */
	-webkit-border-radius: 5px; /*Para Safari e Chrome */
	border-radius: 5px; /* Para Opera 10.5+*/
}

h2,h3 {
	text-align: left;
	margin-left: 30px;
	margin-bottom: 20px;
	font-family: "Source Sans Pro", sans-serif;
	font-weight: 400;
}

table table-striped {
	text-align: center;
}

p {
	text-align: left;
	margin-bottom: 40px;
	font-family: "Source Sans Pro", sans-serif;
	font-weight: 400;
}

h4 {
	color: #fff;
}

h6 {
	color: #fff;
}

.content {
	text-align: center;
	color: #000;
	padding-top: 30px;
}

.question {
	text-align: center;
	padding-top: 30px;
}

i {
	text-align: left;
}

.span3 {
	padding-left: 10px;
}

.nav-body {
	padding-top: 30px;
	text-align: center;
}

.nav-body2 {
	padding-top: 50px;
}

input[type="text"],input[type="password"] {
	padding: 3px;
	width: 250px;
	margin-right: 30px;
}

select {
	padding: 3px;
	width: 263px;
}

input[type="submit"].btn {
	margin-left: 30px;
	margin-right: 20px;
	margin-top: 10px;
	margin-bottom: 10px;
	padding: 5px 30px;
}

label {
	margin-left: 5px;
	margin-right: 35px;
	text-align: left;
}

.ui-widget {
	font-size: 15px;
}

html,body,#map-canvas {
	height: 100%;
	margin: 0px;
	padding: 0px
}

.controls {
	margin-top: 16px;
	border: 1px solid transparent;
	border-radius: 2px 0 0 2px;
	box-sizing: border-box;
	-moz-box-sizing: border-box;
	height: 32px;
	outline: none;
	box-shadow: 0 2px 6px rgba(0, 0, 0, 0.3);
}

#pac-input {
	background-color: #fff;
	padding: 0 11px 0 13px;
	width: 400px;
	font-family: Roboto;
	font-size: 15px;
	font-weight: 300;
	text-overflow: ellipsis;
}

#pac-input:focus {
	border-color: #4d90fe;
	margin-left: -1px;
	padding-left: 14px; /* Regular padding-left + 1. */
	width: 401px;
}

.pac-container {
	font-family: Roboto;
}

#type-selector {
	color: #fff;
	background-color: #4d90fe;
	padding: 5px 11px 0px 11px;
}

#type-selector label {
	font-family: Roboto;
	font-size: 13px;
	font-weight: 300;
}